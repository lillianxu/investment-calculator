{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "always",
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "semi": true,
  "overrides": [
    {
      "files": "*.module.css",
      "options": {
        "parser": "css"
      }
    }
  ],
  "fileExtensions": [".module.css"]
}
